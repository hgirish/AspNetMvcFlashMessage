<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/flashhelper.css")" rel="stylesheet" type="text/css" />
</head>
  <body>
    <ul>
      <li>@Html.ActionLink("Info","Info","Home")</li>
      <li>@Html.ActionLink("Warning","Warning","Home")</li>
      <li>@Html.ActionLink("Error","Error","Home")</li>
    </ul>
    @RenderBody()
    
    
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js"></script>

    <div id="flash" style="display: none"></div>
    @Flash.FlashMessage(TempData)

  </body>
</html>
